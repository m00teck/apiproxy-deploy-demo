<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<RaiseFault continueOnError="false" enabled="true" name="RF-NotFound">
  <DisplayName>RF-NotFound</DisplayName>
  <FaultResponse>
    <Set>
      <Headers>
        <Header name="error-flow">404 Not Found</Header>
      </Headers>
      <Payload contentType="application/json">{"message": "Resource not found on this server"}</Payload>
      <StatusCode>404</StatusCode>
      <ReasonPhrase>Not Found</ReasonPhrase>
    </Set>
  </FaultResponse>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
</RaiseFault>
