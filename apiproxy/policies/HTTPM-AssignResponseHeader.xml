<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<HTTPModifier continueOnError="false" enabled="true" name="HTTPM-AssignResponseHeader">
  <DisplayName>HTTPM-AssignResponseHeader</DisplayName>
  <Properties/>
  <Add>
    <Headers>
      <Header name="X-Hello-Apigee-In-Response">Hello From the Response Header!</Header>
    </Headers>
  </Add>
  <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
  <AssignTo createNew="false" transport="http" type="response"/>
</HTTPModifier>
